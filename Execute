local function LocalScript_4(script)

	--[[
	
	
	
	            __   ___     __             ___  __   __       
	 |\/|  /\  |  \ |__     |__) \ /    \_/  |  /  \ |__)  /\  
	 |  | /~~\ |__/ |___    |__)  |     / \  |  \__/ |    /~~\ 
	                                                           
	
	
