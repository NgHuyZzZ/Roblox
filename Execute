local function LocalScript_2(script)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(script.Parent.Parent.Source.Text)()
	end)
	
end

----------------------------------------------------------------------------------------------------

local function LocalScript_3(script)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Source.Text = ''
	end)
	
end

----------------------------------------------------------------------------------------------------

local function LocalScript_8(script)

	--You dont need to delete this text.
	--PUT INSIDE a BUTTON in StarterGui
	--Make sure to make a frame for the menu to pop up then go to properties > Visible [Off]
	script.Parent.MouseButton1Click:Connect (function()
		script.Parent.Parent.Main.Visible = not script.Parent.Parent.Main.Visible
	end)
end

