local function LocalScript_1(script)

	--[[
	
	
	
	            __   ___     __             ___  __   __       
	 |\/|  /\  |  \ |__     |__) \ /    \_/  |  /  \ |__)  /\  
	 |  | /~~\ |__/ |___    |__)  |     / \  |  \__/ |    /~~\ 
	                                                           
	
	
			~ 2023.03.31
		- Moving UIGradient
	
	]]--
	
	-- Variables 
	
	local TweenService = game:GetService("TweenService")
	local gradient = script.Parent
	
	-- Main Func.
	
	while true do
		local tween = TweenService:Create(gradient, TweenInfo.new(1), {
			Offset = Vector2.new(0.3 * math.sin(time()), 0)
		})
		tween:Play()	-- start the tween animation
		wait()	-- A wait for when the gradient reaches the end point, if it was wait(1) it would wait 1 second before looping over again
		
	--REMOVING "WAIT" WILL BREAK THE ENTIRE SCRIPT AND LAG YOUR GAME
		
	end -- end [Can you believe that!]
	
end

